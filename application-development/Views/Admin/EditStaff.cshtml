@model application_development.Models.Staff

<h2>Edit Staff</h2>

@using (Html.BeginForm("EditStaff", "Admin", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
{
    @Html.AntiForgeryToken()
    <hr />
    @Html.ValidationSummary("", new { @class = "text-danger" })
    <div class="form-group">
        @Html.LabelFor(m => m.StaffName, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.StaffName, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            @Html.ActionLink("Back", "AllStaff", "Admin", null, new { @class = "btn btn-default" })
            <input type="submit" class="btn btn-default" value="Save Change" />
        </div>
    </div>
}


